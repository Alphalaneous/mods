{
    "geode": "1.0.3",
    "version": "1.0.1",
    "id": "alk.fpsbypass",
    "name": "FPS Bypass (MacOS)",
    "developer": "alk",
    "description": "Just a simple FPS physics and visual bypass for MacOS. In order to fully enable/disable you need to restart your game.",
    "dependencies": [],
    "early-load": true,
    "settings": {
        "fps": {
            "type": "float",
            "name": "Set FPS",
            "description": "Sets the framerate.",
            "default": 60.0
        },
        "vsync": {
            "type": "bool",
            "name": "Enable VSync",
            "description": "Enables the application VSync",
            "default": true
        }
    }
}
